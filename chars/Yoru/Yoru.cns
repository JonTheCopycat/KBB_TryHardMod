; var(0):= 40 - Jump Start, 45 - AirJump Start, 50 - Jump Up, 132 - Air guard (guarding), 155 - Air guard hit (knocked away), 5080 - Downed get-hit (shaking), 5081 - Downed get-hit (knocked back), 5100 - Downed get-hit (hit ground from fall), 5110 - Downed get-hit (lying down), 1003 - Hard Knockdown - hit ground from fall, 1005 - Hard Knockdown - Lying Down
; var(2):= -3
; var(3):= 45 - AirJump Start, -3
; var(4):= 66 - Instant Airdash:, -3
;var(10):= 490 - Wavedash C
;var(11):= 200 - A - 1, 201 - Anti Air/4A or A - 3, 210 - A - 2, 305 - B - 1: Helper, 315 - B - 2: Helper, 316 - B - Dab: Helper, 410 - C - Start, 430 - Down C:, 440 - Metastar Rush, 702 - GRAB - Metastar, 950 - Low Attack, 951 - Dash Attack, 600 - A - Aire, 610 - B - Aire, 630 - C - Air Helper, -3, 21000 - Rising Slash, 50000 - Star Burst, 51000 - Star Burst Start, 54000 - Star Buster, 54500 - Star Buster off Block, 51001 - Star Burst ground dash proration cooldown, 51002 - Star Burst air dash proration cooldown, -3 - StarBurst Negative Statedef Stuff
;var(17):= 25000 - Love Deluxe
