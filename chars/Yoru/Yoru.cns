persistent = 0

; ------ Defense Set ------

[State -2 Normal Defence] ; If not in tag mode, characters have normal life
type = MapSet
trigger1 = teammode != tag && enemy,teammode != tag
map = "initialDefence"
value = 1
persistent = 0

[State -2, Defense Reset] ; Resets character's defense modifier to 0 at round start
type = MapSet
trigger1 = roundstate = 1
map = "defence"
value = 0

[State -2, Defense Set] ; Used to determine character defense
type = defenceMulSet
trigger1 = 1 
value = map(initialDefence) + map(defence)
onhit = 0

; ------ Tag mode code ------
[State -2, send proration to partners] ;Activates when the partner is switching out
type = TeamMapSet
triggerall = teammode = tag
trigger1 = stateno = 5610
map = "tagProration"
value = var(11)

[State -2, Take proration from partners] ; Takes proration from switching out character
type = varSet
triggerall = teammode = tag
trigger1 = stateno = 5600
v = 11
value = map(tagProration)

[State -2, Use partner's juggle]
type = TeamMapSet
triggerall = teammode = tag
trigger1 = movehit || helper,movehit
map = "juggle"
value = map(juggle)

[State -2, Reduce juggle on first tag] ; Cuts juggle in half when you tag in a character for the first time in a combo
type = MapSet
triggerall = teammode = tag
trigger1 = stateno = 5600
trigger1 = map(tagSwitch) = 0
map = "juggle"
value = map(juggle) / 2

[State -2, tagSwitch] ; Prevents the player from reducing juggle by switching to the same character multiple times in one combo
type = MapSet
triggerall = teammode = tag
trigger1 = stateno = 5600
map = "tagSwitch"
value = 1

[State -2, Reset tagSwitch]
type = TeamMapset
trigger1 = numenemy
trigger1 = (enemynear,movetype!=H && enemy,stateno != 52)
trigger2 = !numenemy
map = "tagSwitch"
value = 0
ignorehitpause = 1
persistent = 0

; Sets character defense based on team size (1 character = 200% defense, 2 character = 100%, 3 = 66%, 4 = 50%)
; This has to be spread out across multiple states because of an Ikemen bug (Fixed in 0.98.2)

[State -2 Normal Defence] 
type = MapSet
trigger1 = teammode != tag || enemy,teammode != tag
map = "initialDefence"
value = 1
persistent = 0

[State -2 Tag Defence Solo] 
type = MapSet
triggerall = teammode = tag || enemy,teammode = tag
trigger1 = numPartner = 0
map = "initialDefence"
value = 0.5 
persistent = 0

[State -2 Tag Defence Duo] 
type = MapSet
triggerall = teammode = tag || enemy,teammode = tag
trigger1 = numPartner = 1
map = "initialDefence"
value = 1 
persistent = 0

[State -2 Tag Defence Trio] 
type = MapSet
triggerall = teammode = tag || enemy,teammode = tag
trigger1 = numPartner = 2
map = "initialDefence"
value = 1.5 - (ifelse(enemy,numPartner = 2,0.3,0)) - (ifelse(enemy,numpartner = 3,0.3,0)) ; 666hp, 833 in 3v3 and 4v4
persistent = 0

[State -2 Tag Defence Quadro] 
type = MapSet
triggerall = teammode = tag || enemy,teammode = tag
trigger1 = numPartner = 3
map = "initialDefence"
value = 2 - (ifelse(enemy,numPartner = 2,0.4,0)) - (ifelse(enemy,numpartner = 3,0.6,0)) ; 500 hp, 625 in 4v3 and 700 in 4v4
persistent = 0

