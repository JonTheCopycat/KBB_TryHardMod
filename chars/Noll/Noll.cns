; var(0):= 1003 - Hard Knockdown - hit ground from fall, 1005 - Hard Knockdown - Lying Down, 40 - Jump Start, 45 - AirJump Start, 50 - Jump Up, 132 - Air guard (guarding), 155 - Air guard hit (knocked away), 5080 - Downed get-hit (shaking), 5081 - Downed get-hit (knocked back), 5100 - Downed get-hit (hit ground from fall), 5110 - Downed get-hit (lying down)
; var(3):= -3, 45 - AirJump Start
; var(4):= -3, 66 - Instant Airdash:
;var(11):= 200 - A - 1, 250 - This is the first hit of A - 2, 260 - This is the second hit of A - 2, 305 - B Helper, 320, 321, 322, 323, 324, 325, 326, 327, 350 - Back+B Anti-Air, 452 - C Helper Spiral End, 700 - Grab, 901 - "Overhead Helper", 950 - Low Attack, 600 - A - Aire, 610 - B - Aire, 401 - Down + C Helper Phantom Blade - SPAWN AND IDLE, 407 - Down + C Direction Stab - Horizontal, 410 - Down + C Direction Slash - Down, 411 - Down + C Direction Slash - Up, -3, 22000 - Rapidslash DP, 23000 - Spinner Cutters:, 24000 - Sleeper Move:, 25555 - Counter Cutter Phantom Blade Final Slice, 50000 - Star Burst, 51000 - Star Burst Start, 54000 - Star Buster, 54500 - Star Buster off Block, 51001 - Star Burst ground dash proration cooldown, 51002 - Star Burst air dash proration cooldown, -3 - StarBurst Negative Statedef Stuff
;var(12):= 305 - B Helper, -3
;var(29):= -3 - Dont mess with; Executed when in self state
;var(30):= -3
;var(35):= 301 - BB - Stance, 310 - Down+Back, 311 - Down, 312 - Down+Fwd, 313 - Back, 314 - Fwd, 315 - Up+Fwd, 316 - Up, 317 - Up+Back, 470 - Down + C, -3
