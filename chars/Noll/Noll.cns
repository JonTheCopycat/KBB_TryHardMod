[State 0, StopSnd]
type = StopSnd
trigger1 = movetype = H
trigger2 = stateno != 500
channel = 6

;This controller plays a sound everytime the player lands from a jump, or
;from a back-dash.
[State -3, Landing Sound]
type = PlaySnd
triggerall = Time = 1
trigger1 = stateno = 52 ;Jump land
trigger2 = stateno = 106 ;Run-back land
value = 40, 0

[State 0, StopSnd]
type = StopSnd
trigger1 = movetype = H
trigger2 = stateno != 500
channel = 6

[State 0, Helper]
type = Helper
triggerall = numhelper(666) = 0
triggerall = roundstate = 2 || roundstate = 3
triggerall = hitover = 0
trigger1 = life = 0
helpertype = normal ;player
name = "Death Sound"
ID = 666
stateno = 666
pos = 9999,9999
postype = p1    ;p2,front,back,left,right
facing = 1
keyctrl = 0
ownpal = 1
supermovetime = 9999
pausemovetime = 9999


[State 0, VarSet]
type = VarSet
trigger1 = pos Y >= 0 
v = 4    ;fv = 10
value = 0

[State 0, VarSet]
type = VarSet
trigger1 = pos Y >= 0 
v = 3    ;fv = 10
value = 0

[State 0, VarSet]
type = VarSet
trigger1 = roundstate = 0
v = 12    ;fv = 10
value = 0

; -------- Helper Power Gain -------------
;If Noll hits with one of his non-meter consuming helpers, gives him meter
[State -2 Noll Power Gain]
type = powerAdd
triggerall = P2Movetype = H
triggerall = enemy,time = 1
triggerall = numhelper(51000) = 0
triggerall = numhelper(55000) = 0
triggerall = numhelper(56000) = 0
trigger1 = helper(305), movecontact = 1 
trigger2 = helper(901), movecontact = 1 && P2stateno != 5001
value = 50

;--------------------------------------

; ------ Damage scaling -------
[State -2, Attack Scale]
type = AttackMulSet
trigger1 = 1
value = 1 * 0.99**var(11)
ignorehitpause = 1

[State -2, Counterhit TSP]
type = Helper
Triggerall = numhelper(4020) = 0
trigger1 = movecountered
helpertype = normal 
name = "Counterhit TSP"
supermovetime = 999999999999
pausemovetime = 999999999999
ignorehitpause = 1
persistent = 0
ID = 4020
stateno = 4020
postype = p1    ;p2,front,back,left,right
facing = 1
keyctrl = 0
ownpal = 1
ignorehitpause = 1
persistent = 0

[State -2, Reset proration when the opponent recovers]
type = Varset
trigger1 = numenemy
trigger1 = (enemynear,movetype!=H && enemy,stateno != 52)
trigger2 = !numenemy
var(11) = ifelse(numhelper(51000)!=0,-20,0) ; Sets proration to -20 if in starburst mode, otherwise resets to 0
ignorehitpause = 1
persistent = 0

[State -2, Reset Juggle when the opponent recovers]
type = MapSet
trigger1 = numenemy
trigger1 = (enemynear,movetype!=H && enemy,stateno != 52)
trigger2 = !numenemy
map = "juggle"
value = ifelse(numhelper(51000) !=0,-25,0) ; Sets juggle to -25 if in starburst mode, otherwise resets to 0
ignorehitpause = 1
persistent = 0

; ------ Defense Set ------

[State -2 Normal Defence] ; If not in tag mode, characters have normal life
type = MapSet
trigger1 = teammode != tag && enemy,teammode != tag
map = "initialDefence"
value = 1
persistent = 0

[State -2, Defense Reset] ; Resets character's defense modifier to 0 at round start
type = MapSet
trigger1 = roundstate = 1
map = "defence"
value = 0

[State -2, Defense Set] ; Used to determine character defense
type = defenceMulSet
trigger1 = 1 
value = map(initialDefence) + map(defence)
onhit = 0

; ------ FatalCounter Check ------
[State -2, FatalCounterCheck]
type = MapSet
triggerall = map(fatal) = 1
trigger1 = numenemy
trigger1 = (enemynear,movetype!=H && enemy,stateno != 52)
trigger2 = !numenemy
map = "fatal"
value = 0

; ------ HitOverride Check ------
[State -2, HitOverrideCheck]
type = MapSet
trigger1 = movetype = H
trigger2 = stateno != [800,803] && stateno != [810,813] && stateno != 830 ;Universal counters
trigger2 = stateno != 25000 ;236c counter
map = "hitOverride"
value = 0

; -------- Busterable check --------
[State -2, Busterablecheck]
type = MapSet
triggerall = numhelper(451) = 0
trigger1 = movetype = H
trigger2 = stateno != 700
trigger2 = stateno != 450 && stateno != 620
trigger2 = stateno != 21000 && stateno != 22000 && stateno != 23000 && stateno != 24000 
map = "busterable"
value = 0

; -------- Tagging & DHC ------
[State -2, enableDHC] ; Disables DHC if not in a special move state
type = MapSet
triggerall = teammode = tag
triggerall = map(enableDHC) = 1
trigger1 = movetype = H
trigger2 = stateno != [20000,29999]
map = "enableDHC"
value = 0

[State -2, EnableTag] ; Renables tagging in
type = MapSet
triggerall = teammode = tag
trigger1 = command != "SPECIAL 1" && command != "SPECIAL 2" && command != "SPECIAL 3" && command != "SPECIAL 4" && command != "SPECIAL 5" && command != "SPECIAL 6"
map = "disableTag"
value = 0

[State -2, DisableTagBuffer] ; If the player buffers a special move, disables tagging
type = MapSet
triggerall = teammode = tag
trigger1 = command = "SPECIAL 1" || command = "SPECIAL 2" || command = "SPECIAL 3" || command = "SPECIAL 4" || command = "SPECIAL 5" || command = "SPECIAL 6"
map = "disableTag"
value = 1
